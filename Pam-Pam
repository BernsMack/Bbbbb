#include<iostream>
#include<math.h>
using namespace std;

void main(){
cout<<"Hello world!"<<endl;
int a,b,c,x1,x2,d;
cout<<"ax^2+bx+c=0"<<endl;
cout<<"a = ";
cin >> a;
cout<<endl;
cout<<"b = ";
cin >> b;
cout<<endl;
cout<<"c = ";
cin >> c;
cout<<endl;
d = sqrt(b*b - 4*a*c);
x1 = (b*(-1)-d)/2*a;
x2 = (b*(-1)+d)/2*a;
cout<<"X1 = "<<x1<<" "<<"X2 = "<<x2<<endl;
system("pause");

int ar[10];
	int x1 = 0;
	
	for (int i = 0; i < 10; i++)
		ar[i] = rand()%10;
	int x2 = ar[1];
	for (int i = 0; i < 10; i++) {
		x1 += ar[i];
		x2 *= ar[i];
	}
	cout << x1 << " " << x2 << endl;
	system("pause");
}
